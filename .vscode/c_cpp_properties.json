{
    "env": {
        "myDefaultIncludePath": [
            "${workspaceFolder}/**"
        ]
    },
    "configurations": [
        {
            // "name": "ARM",
            "name": "Linux",
            // "intelliSenseMode": "gcc-arm",
            "intelliSenseMode": "linux-gcc-arm",
